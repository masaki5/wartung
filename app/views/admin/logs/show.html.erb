<div class="row">
  <div class="col-sm-6">
    <h1>履歴内容</h1>
    <table class="table table-hover table-inverse table-bordered">
      <thead class="thead-light">
        <tr>
          <th>交換部品</th>
          <th>交換日</th>
          <th>交換推奨日</th>
          <th>交換距離</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= @log.part.name %></td>
          <td><%= @log.exchange %></td>
          <td><%= @log.next %></td>
          <td><%= number_to_currency(@log.after) %></td>

        </tr>
      </tbody>
    </table>

    <%= render partial: 'admin/logs/edit', locals: { user: @user, car: @car, part: @part, log: @log} %>
  </div>
</div>