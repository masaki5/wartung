<h1>New Part</h1>

<%= form_for(@part, url: '/admin/parts') do |f| %>
	<div>
		<%= f.label "部品名" %>
		<%= f.text_field :name %>
	</div>
    <div class="field">
    <%= f.label "商品説明" %>
    <%= f.text_field :introduction %>
  </div>


	<div class="actions row">
		<%= f.submit "車両登録" %>
	</div>
  <% end %>

<%= link_to 'Back', admin_parts_path %>