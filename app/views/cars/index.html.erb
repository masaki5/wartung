  <div class="row">
    <div class="col-sm-8">
      <h1>登録車両一覧</h1>

      <table class="table table-hover table-inverse table-bordered">
        <thead class="thead-light">
          <tr>
            <th>車両名</th>
            <th>編集</th>
            <th>削除</th>
          </tr>
        </thead>
        <tbody id="cars-list">
          <%= render partial: 'cars/table', locals: { cars: @cars} %>
        </tbody>
      </table>
      <%= paginate @cars %>
      <button type="button" class="btn btn-primary btn-md">
        <%= link_to '戻る', :back, :style=>"color:white;" %></button>
    </div>
    <div class="col-sm-4">
      <div id="car">
        <%= render partial: 'cars/new', locals: { car: @car} %>
      </div>
    </div>
  </div>