<p id="notice"><%= notice %></p>

<h1>登録車両</h1>

<table>
  <thead>
    <tr>
      <!--<th>車両名</th>-->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |c| %>
      <tr>
        <td><%= link_to c.name, car_parts_path(c.id) %></td>
        <td><%= link_to 'Show', car_path(c.id) %></td>
        <td><%= link_to 'Edit', edit_car_path(c.id) %></td>
        <td><%= link_to 'Destroy', car_path(c.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

