<div class="row">
  <div class="col-sm-6">
    <p id="notice"><%= notice %></p>
    <h1>履歴内容</h1>
    <table class="table table-hover table-inverse table-bordered">
      <thead>
        <tr>
          <th>交換部品</th>
          <th>交換日</th>
          <th>交換推奨日</th>
          <th>交換距離</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= @log.part.name %></td>
          <td><%= @log.exchange %></td>
          <td><%= @log.next %></td>
          <td><%= number_to_currency(@log.after) %></td>

        </tr>
      </tbody>
    </table>

    <%= render partial: 'logs/edit', locals: { car: @car, log: @log} %>
  </div>
  <div class="col-sm-6">

    エラー文表示



  </div>
</div>