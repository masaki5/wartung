<h1>New Part</h1>

<%= form_for(@part) do |f| %>
<div class="field row">
    <%= f.label "部品名" %>
    <%= f.text_field :name %>
</div>
<div class="field row">
    <%= f.label "商品説明" %>
    <%= f.text_area :introduction %>
</div>

<div class="actions row">
    <%= f.submit "新規登録" %>
</div>
<% end %>


<%= link_to 'Back', parts_path %>